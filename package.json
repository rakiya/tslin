{
  "name": "@rakiya/tslin",
  "version": "0.0.16",
  "description": "This package add rich methods like Kotlin to your project.",
  "main": "src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --silent=false",
    "release": "npm publish",
    "prerelease": "npm run build",
    "postrelease": "find ./src -name '*.d.ts' -delete -or -name '*.js' -delete -or -name '*.js.map' -delete && find ./tests -name '*.d.ts' -delete -or -name '*.js' -delete -or -name '*.js.map' -delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rakiya/tslin.git"
  },
  "author": "rakiya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rakiya/tslin/issues"
  },
  "homepage": "https://github.com/rakiya/tslin#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.1",
    "jsonc-parser": "^3.2.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
